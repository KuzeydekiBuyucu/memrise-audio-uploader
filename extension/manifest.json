{  "manifest_version": 2,  "name": "Memrise Audio Uploader",  "description": "Auto uploads audio for words in your course",  "version": "1.0",  "icons": {    "128": "images/mainIcon128.png"  },  "background": {    "scripts": [      "jquery.js",      "messages.js",      "soundOfText.js"    ],    "persistent": true  },  "content_scripts": [{    "js": [      "jquery.js",      "memriseCourse.js",      "messages.js",      "uploadManager.js",      "autoUploadBtn.js"    ],    "matches": ["https://www.memrise.com/course/*/edit/*"],    "run_at": "document_end"  }],  "web_accessible_resources": [    "pageInjectedScript.js"  ],  "permissions": [    "http://soundoftext.com/*"  ]}