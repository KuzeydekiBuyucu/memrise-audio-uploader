{  "manifest_version": 2,  "name": "Memrise Audio Uploader",  "description": "Auto uploads audio for words in your course",  "version": "1.0",  "icons": {    "128": "images/mainIcon128.png"  },  "background": {    "scripts": [      "jquery.js",      "soundOfText.js",      "require.js"    ],    "persistent": true  },  "content_scripts": [{    "js": [      "jquery.js",      "memriseEditCourseContent.js",      "uploadManager.js",      "injectedButton.js",      "require.js"    ],    "matches": ["https://www.memrise.com/course/*/edit/*"]  }],  "web_accessible_resources": ["pageInjectedScript.js"],  "permissions": [    "webRequest",    "http://soundoftext.com/*"  ]}