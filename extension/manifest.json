{  "manifest_version": 2,  "name": "Memrise Audio Uploader",  "description": "Auto uploads audio for words in your course",  "version": "1.0",  "icons": {    "128": "images/mainIcon128.png"  },  "background": {    "scripts": [      "messages.js",      "chromeUtils.js",      "background.js"    ],    "persistent": true  },  "content_scripts": [{    "js": [      "jquery-2.2.3.js",      "memriseEditCourseContent.js",      "injectedButton.js",      "messages.js"    ],    "matches": ["https://www.memrise.com/course/*/edit/*"]  }],  "web_accessible_resources": ["pageInjectedScript.js"],  "page_action": {    "default_icon": "images/pageActionIcon38.png",    "default_popup": "popup.html",    "default_title": "Memrise Audio Uploader"  },  "permissions": [    "activeTab",    "tabs",    "webRequest",    "downloads",    "*://*/GMapsLocationController?Action=display*"  ]}