{  "manifest_version": 2,  "name": "Memrise Audio Uploader",  "description": "Auto uploads audio for words in your course",  "version": "1.1.0",  "icons": {    "128": "images/mainIcon128.png"  },  "background": {    "scripts": [      "jquery.min.js",      "messages.js",      "soundOfText/soundOfText.js"    ],    "persistent": true  },  "content_scripts": [{    "js": [      "jquery.min.js",      "memrise/memriseCourse.js",      "memrise/memriseCourseWordRow.js",      "messages.js",      "uploadManager.js",      "soundOfText/languages.js",      "autoUploadBtn.js"    ],    "matches": [      "https://www.memrise.com/course/*/edit/*"    ],    "run_at": "document_end"  }],  "web_accessible_resources": [    "pageInjectedScript.js"  ],  "permissions": [    "https://www.memrise.com/*",    "https://api.soundoftext.com/*",    "https://*.digitaloceanspaces.com/*"  ]}